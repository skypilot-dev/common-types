{
  "name": "@skypilot/common-types",
  "description": "Library of reusable common types for TypeScript projects",
  "homepage": "https://github.com/skypilotcc/common-types#readme",
  "version": "0.1.1",
  "main": "lib/index.js",
  "author": {
    "email": "wthorsen@skypilot.dev",
    "name": "William Thorsen"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skypilotcc/common-types.git"
  },
  "bugs": {
    "url": "https://github.com/skypilotcc/common-types/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "@skypilot/toolchain": "^0.4.21",
    "bumped": "^0.12.0",
    "bumped-changelog": "^0.3.12",
    "bumped-finepack": "^0.3.0",
    "bumped-terminal": "^0.7.8"
  },
  "files": [
    "/lib"
  ],
  "scripts": {
    "all-ci-checks": "yarn run all-cq-checks && yarn run build",
    "all-cq-checks": "yarn run check-types && yarn run lint --quiet && yarn test",
    "build": "rm -rf lib && yarn run compile-ts",
    "check-types": "tsc",
    "compile-ts": "babel ./src --out-dir ./lib --extensions .ts --ignore '**/*.test.ts'",
    "generate-typings": "tsc --project tsconfig.generate-typings.json",
    "lint": "eslint --cache '**/*.{js,ts}'",
    "prepublishOnly": "yarn run check-types && yarn run lint --quiet && yarn test && yarn run build && yarn run generate-typings",
    "test": "jest"
  },
  "private": false,
  "license": "ISC",
  "types": "lib/index.d.ts"
}